generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Todo {
  id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title     String
  completed Boolean  @default(false)
  createdAt DateTime @default(now())
}
